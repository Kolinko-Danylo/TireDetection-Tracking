   "source": [
    "import numpy as np\n",
    "import cv2\n",
    "\n",
    "def auto_canny(image, sigma=1):\n",
    "    v = np.median(image)\n",
    "  \n",
    "    lower = int(max(0, (1.0 - sigma) * v))\n",
    "    upper = int(min(255, (1.0 + sigma) * v))\n",
    "    edged = cv2.Canny(image, lower, upper)\n",
    " \n",
    "    return edged"
   ]
  }
